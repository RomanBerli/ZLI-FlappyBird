<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/scoreTextView"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:alpha="0"
            android:translationY="10sp"
            android:scaleX="1"
            android:scaleY="1" />
        <Constraint
            android:id="@+id/button2"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.191"
            motion:layout_constraintTop_toBottomOf="@+id/button3"
            motion:layout_constraintHorizontal_bias="0.498"
            android:scaleX="1"
            android:scaleY="1"
            android:translationY="10sp"
            android:alpha="0" />
        <Constraint
            android:id="@+id/button3"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.408"
            motion:layout_constraintTop_toBottomOf="@+id/scoreTextView"
            motion:layout_constraintHorizontal_bias="0.498"
            android:scaleX="1"
            android:translationY="10sp"
            android:alpha="0" />
        <Constraint
            android:id="@+id/textView"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toTopOf="@+id/button3"
            motion:layout_constraintVertical_bias="0.268"
            motion:layout_constraintTop_toBottomOf="@+id/scoreTextView"
            motion:layout_constraintHorizontal_bias="0.498"
            android:scaleX="1"
            android:scaleY="1"
            android:translationY="10sp"
            android:alpha="0" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
    </ConstraintSet>
</MotionScene>